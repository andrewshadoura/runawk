#!/usr/bin/env runawk

#use "quicksort.awk"

# This demo sorts the input lines as (key, value) pair
# and outputs sorted pairs.

# Input file for this demo: examples/demo_heapsort3.in

{
	hash [$1] = $2
}

END {
	count = quicksort_values(hash, remap)

	for (i=1; i <= count; ++i){
		print remap [i], hash [remap [i]]
	}
}
