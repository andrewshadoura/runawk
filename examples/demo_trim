#!/usr/bin/env runawk

#use "trim.awk"

# This is a demo for trim.awk module

# Input files for this demo: examples/demo_trim.in

{
	printf "trim_l(\"%s\")=\"%s\"\n", $0, trim_l($0)
	printf "trim_r(\"%s\")=\"%s\"\n", $0, trim_r($0)
	printf "trim_c(\"%s\")=\"%s\"\n", $0, trim_c($0)
	printf "trim_lr(\"%s\")=\"%s\"\n", $0, trim_lr($0)
	printf "trim_lrc(\"%s\")=\"%s\"\n", $0, trim_lrc($0)
	printf "trim(\"%s\")=\"%s\"\n", $0, trim($0)
}
